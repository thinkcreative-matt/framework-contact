{
    "name": "thinkcreative/contact",
    "description": "This will allow us to to view and administer the contact information section of the framework",
    "authors": [
        {
            "name": "Matthew Butler",
            "email": "matthew@thinkcreative.uk.com"
        }
    ],
    "require": {
        "laravelcollective/html": "^6.0"
    },
    "autoload": {
        "psr-0": {
            "Thinkcreative\\Contact\\": "src"
        },
        "files": [
            "src/Thinkcreative/Contact/functions.php"
        ]
    },
    "extra" : {
        "laravel": {
            "providers" : [
                "Thinkcreative\\Contact\\ContactServiceProvider"
            ],
            "aliases": {
                "Contact" : "Thinkcreative\\Contact\\Contact"
            }
        }
    }
}
